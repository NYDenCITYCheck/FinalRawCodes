   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Creating data frames for weeks ending Julye 4th, 2020 through July 25th, 2020 (four weeks)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "week1_url = 'http://web.mta.info/developers/data/nyct/turnstile/turnstile_200704.txt'\n",
    "week2_url = 'http://web.mta.info/developers/data/nyct/turnstile/turnstile_200711.txt'\n",
    "week3_url = 'http://web.mta.info/developers/data/nyct/turnstile/turnstile_200718.txt'\n",
    "week4_url = 'http://web.mta.info/developers/data/nyct/turnstile/turnstile_200725.txt'\n",
    "\n",
    "col_name = ['C/A', 'Unit', 'SCP', 'Station', 'Line Name', \n",
    "            'Division', 'Date', 'Time', 'Description', 'Entries', 'Exits']\n",
    "\n",
    "week1 = pd.read_csv(week1_url, sep=\",\", header=0, names = col_name)\n",
    "week2 = pd.read_csv(week2_url, sep=\",\", header=0, names = col_name)\n",
    "week3 = pd.read_csv(week3_url, sep=\",\", header=0, names = col_name)\n",
    "week4 = pd.read_csv(week4_url, sep=\",\", header=0, names = col_name)\n"
   ]
